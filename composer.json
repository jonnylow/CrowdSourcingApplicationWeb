{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/missbossy/Faker.git"
        },
        {
            "type": "package",
            "package": {
                "name": "fronteed/icheck",
                "version": "1.0.2",
                "dist": {
                    "url": "https://github.com/fronteed/icheck/archive/1.0.2.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/fronteed/icheck.git",
                    "type": "git",
                    "reference": "origin/1.x"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "rriepe/1pxdeep",
                "version": "1.0.1",
                "dist": {
                    "url": "https://github.com/rriepe/1pxdeep/archive/master.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/rriepe/1pxdeep.git",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.1.*",
        "laravelcollective/html": "5.1.*",
        "tymon/jwt-auth": "^0.5.5",
        "guzzlehttp/guzzle": "~5.3|~6.0",
        "thomaspark/bootswatch": "^3.3.5",
        "fronteed/icheck": "1.0.2",
        "wenzhixin/bootstrap-table": "^1.9.1",
        "rriepe/1pxdeep": "1.0.1"
    },
    "require-dev": {
        "fzaninotto/faker": "dev-singapore",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
